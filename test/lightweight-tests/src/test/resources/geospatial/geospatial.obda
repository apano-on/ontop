[PrefixDeclaration]
:		http://ex.org/
xsd:    http://www.w3.org/2001/XMLSchema#
geo:    http://www.opengis.net/ont/geosparql#
geof:   http://www.opengis.net/def/function/geosparql/
rdfs:   http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	geoms
target		:{"id"} a :Geom ; geo:asWKT {"geom"}^^geo:wktLiteral ; rdfs:label {"name"}^^xsd:string .
source		SELECT * FROM "GEOMS";

mappingId	geogs
target		:{"id"} a :Geog ; geo:asWKT {"geog"}^^geo:wktLiteral ; rdfs:label {"name"}^^xsd:string .
source		SELECT * FROM "GEOGS";

mappingId	geoms_2
target		:{"id"} a :Geom2 ; geo:asWKT {"geom"}^^geo:wktLiteral ; rdfs:label {"name"}^^xsd:string .
source		SELECT * FROM "GEOMS_2";
]]

